<h1>유저명단</h1>
        <table class="table table-sm table-bordered" style="width: 600px">
            <%
                var i =0;
                while(i<results.length){
            %>
                    <tr><td class="td td-sm"><%=results[i].loginid%></td><td><%=results[i].password%>
                    </td><td><%=results[i].name%></td><td><%=results[i].address%></td>
                    <td><%=results[i].tel%></td><td><%=results[i].class%></td>
                    <td><%=results[i].birth%></td><td><%=results[i].grade%></td>
                    <td><a href="/user/update/<%=results[i].loginid%>">update</a></td>
                    <td><a href="/user/delete_process/<%=results[i].loginid%>">delete</a></td>
                    </tr>
            <%      <!--results[i].title변수니까 '='까지 붙임-->
                    i++;
                }
            %>
        </table>