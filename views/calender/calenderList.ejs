
<h1>월별 계획</h1>
    <table class="table table-sm table-bordered" style="width: 600px">
        <%
        var i=0;
        while(i<results.length){
        %>
            <tr>
                <td class="td td-sm"><%=results[i].title%></td>
                <td><%=results[i].description%></td>
                <td><a href="/calender/update/<%=results[i].id%>">update</a></td>
                <td><a href="/calender/delete_process/<%=results[i].id%>">delete</a></td>
            </tr>
        <%
            i++;
        }
        %>
    </table>
